<!DOCTYPE html>
<html lang="en">

<body class="page-header-fixed page-sidebar-closed-hide-logo page-content-white">

    <form ng-controller="tenant.views.job_information.index as vm" class="form-horizontal" action="#" id="submit_form" method="POST">

        <div class="row">
            <div class="col-md-12">
                <div class="tabbable-line boxless tabbable-reversed">
                   
                    <div class="tab-content">
                        <div class="tab-pane active" id="tab_1">
                            <div class="portlet box green">
                                <div class="portlet-title">
                                    <div class="caption">
                                        <i class="fa fa-gift"></i>Job Details
                                    </div>
                                    <div class="tools">
                                        <a href="javascript:;" class="collapse"> </a>
                                        <a href="#portlet-config" data-toggle="modal" class="config"> </a>
                                        <a href="javascript:;" class="reload"> </a>
                                        <a href="javascript:;" class="remove"> </a>
                                    </div>
                                </div>
                                <div class="portlet-body form">
                                    <!-- BEGIN FORM-->

                                    <div class="form-body">
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Job Status</label>
                                            <div class="col-md-4">
                                                <select data-ng-options="source.name for source in jobStatusList" data-ng-model="selectedJobStatus"  class="form-control">
                                                    <option value="">--Select Status--</option>
                                                </select>                                              
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Tow/DriveIN</label>
                                            <div class="col-md-4">
                                                <select data-ng-options="source.name for source in branchEntryMethodList" data-ng-model="selectedBranchEntry" class="form-control">
                                                    <option value="">--Select Entry Method--</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">New/Warantee</label>
                                            <div class="col-md-4">
                                                <select data-ng-options="source.name for source in new_ComebackList" data-ng-model="selectedNew_Comeback" class="form-control">
                                                    <option value="">--Select Entry Method--</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Claims Handler</label>
                                            <div class="col-md-4">
                                                <select data-ng-options="source.name for source in claimHandlerList" data-ng-model="selectedClaimHandler" class="form-control">
                                                    <option value="">@L("SelectOne")</option>
                                                </select> 
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Service Advisor</label>
                                            <div class="col-md-4">
                                                <select data-ng-options="source.name for source in CSAList" data-ng-model="selectedCSA" class="form-control">
                                                    <option value="">@L("SelectOne")</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Estimator</label>
                                            <div class="col-md-4">
                                                <select data-ng-options="source.name for source in EstimatorList" data-ng-model="selectedEstimator" class="form-control">
                                                    <option value="">@L("SelectOne")</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Parts Buyer</label>
                                            <div class="col-md-4">
                                                <select data-ng-options="source.name for source in PartsBuyerList" data-ng-model="selectedPartsBuyer" class="form-control">
                                                    <option value="">@L("SelectOne")</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Shop Allocation</label>
                                            <div class="col-md-4">
                                                <select required class="form-control" ng-options="" ng-model="">
                                                    <option value="">--Select--</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Accident Details</label>
                                            <div class="col-md-4">
                                                <textarea cols="2" rows="3" class="form-control input-circle"></textarea>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Accident Area</label>
                                            <div class="col-md-4">
                                                <textarea cols="2" rows="3" class="form-control input-circle"></textarea>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Other Repairs</label>
                                            <div class="col-md-4">
                                                <textarea cols="2" rows="3" class="form-control input-circle"></textarea>
                                            </div>
                                        </div>
                                       
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">High Prority</label>
                                            <div class="col-md-4">
                                                <div class="input-group" style="width: 10% !important;">
                                                    <input type="checkbox" class="form-control" />
                                                </div>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Contents bag onsite</label>
                                            <div class="col-md-4">
                                                <div class="input-group" style="width: 10% !important;">
                                                    <input type="checkbox" class="form-control" />
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                    <div class="form-actions">
                                        <div class="row">
                                            <div class="col-md-offset-3 col-md-9">
                                                <button type="button" class="btn btn-circle green button-submit">Submit</button>
                                            </div>
                                        </div>
                                    </div>

                                    <!-- END FORM-->
                                </div>
                            </div>

                        </div>

                    </div>
                </div>
            </div>
        </div>

        <div id="myModal_autocomplete" class="modal fade" role="dialog" aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header" style="background-color:lightgray;">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                        <h4 class="modal-title">Changes Alert</h4>
                    </div>
                    <div class="modal-body form">
                        <label style="margin:15px 0px 0px 15px;">Do you want to save changes you made in this page?</label>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary" data-dismiss="modal">Continue without saving</button>
                        <button type="button" class="btn btn-primary button-submit">
                            <i class="fa fa-check"></i> Save changes
                        </button>
                    </div>
                </div>
            </div>
        </div>

        <div ui-view class="fade-in-up"></div>


    </form>   

    <script>
        $(document).ready(function () {
            $('#clickmewow').click(function () {
                $('#radio1003').attr('checked', 'checked');
            });
        })
    </script>
</body>
</html>