<!DOCTYPE html>
<html lang="en">

<body class="page-header-fixed page-sidebar-closed-hide-logo page-content-white">

    <form ng-controller="tenant.views.uploads.index as vm" class="form-horizontal" action="#" id="submit_form" method="POST">

        <div class="row">
            <div class="col-md-12">
                <div class="tabbable-line boxless tabbable-reversed">
                   
                    <div class="tab-content">
                        <div class="tab-pane active" id="tab_1">
                            <div class="portlet box green">
                                <div class="portlet-title">
                                    <div class="caption">
                                        <i class="fa fa-gift"></i>Uploads
                                    </div>
                                    <div class="tools">
                                        <a href="javascript:;" class="collapse"> </a>
                                        <a href="#portlet-config" data-toggle="modal" class="config"> </a>
                                        <a href="javascript:;" class="reload"> </a>
                                        <a href="javascript:;" class="remove"> </a>
                                    </div>
                                </div>
                                <div class="portlet-body form">
                                    <!-- BEGIN FORM-->

                                    <div class="form-body">

                                        <div class="form-group">

                                            <label style="margin: 0px 0px 0px -55px;" class="col-md-2 control-label">Docs uploads</label>
                                            <div class="col-md-4" style="margin: 6px 0px 0px 0px; width: 31% !important;">
                                                <div class="input-group">
                                                    <input type="file" nv-file-select uploader="vm.job.uploader">
                                                </div>
                                            </div>
                                            <button style="padding: 2px 14px 3px 12px !important; margin: 5px 0px 0px 0px;" type="button" button-busy="vm.saving" busy-text="@L("SavingWithThreeDot")" class="btn btn-primary blue" ng-click="vm.save()" ng-disabled="addinsurerform.$invalid"><i class="fa fa-save"></i> <span>@L("Save")</span></button>

                                        </div>
                                        <div class="form-group"></div>

                                        <div class="form-group">
                                            <label>List of uploads</label>
                                            <hr style="margin: 0px;" />
                                        </div>

                                    </div>
                                    
                                    <!-- END FORM-->
                                </div>
                            </div>

                        </div>

                    </div>
                </div>
            </div>
        </div>

        <div ui-view class="fade-in-up"></div>


    </form>

