<!DOCTYPE html>
<html lang="en">

<body class="page-header-fixed page-sidebar-closed-hide-logo page-content-white">

    <form ng-controller="tenant.views.vehicle_information.index as vm" class="form-horizontal" action="#" id="submit_form" method="POST">

        <div class="row">
            <div class="col-md-12">
                <div class="tabbable-line boxless tabbable-reversed">
          
                    <div class="tab-content">
                        <div class="tab-pane active" id="tab_1">
                            <div class="portlet box green">
                                <div class="portlet-title">
                                    <div class="caption">
                                        <i class="fa fa-gift"></i>Vehicle information
                                    </div>
                                    <div class="tools">
                                        <a href="javascript:;" class="collapse"> </a>
                                        <a href="#portlet-config" data-toggle="modal" class="config"> </a>
                                        <a href="javascript:;" class="reload"> </a>
                                        <a href="javascript:;" class="remove"> </a>
                                    </div>
                                </div>
                                <div class="portlet-body form">
                                    <!-- BEGIN FORM-->

                                    <div class="form-body">
                                        <div class="form-group">
                                            <label class="col-md-2 control-label">Reg No</label>
                                            <div class="col-md-4">
                                                <input required ng-model="vm.vehicle.regNo" ng-class="{'edited':vm.vehicle.regNo}" name="regno" type="text" class="form-control input-block-level" placeholder="Enter Reg no.">
                                            </div>

                                            <label class="col-md-2 control-label">Vin Number</label>
                                            <div class="col-md-4">
                                                <input required ng-model="vm.vehicle.vinNumber" ng-class="{'edited':vm.vehicle.vinNumber}" name="VinNumber" type="text" class="form-control input-block-level" placeholder="Enter Vin Number.">

                                            </div>
                                        </div>
                                       
                                        <div class="form-group">
                                            <label class="col-md-2 control-label">Make</label>
                                            <div class="col-md-4">
                                                <select data-ng-options="source.name for source in MakesList" ng-model="vm.vehicle.selectedMake" class="form-control">
                                                </select>                                               
                                            </div>

                                            <label class="col-md-2 control-label">Model</label>
                                            <div class="col-md-4">
                                                <select data-ng-options="source.name for source in ModelList" ng-model="vm.vehicle.selectedModel" class="form-control"></select>   
                                            </div>

                                        </div>
                                        
                                        <div class="form-group">
                                            <label class="col-md-2 control-label">Colour</label>
                                            <div class="col-md-4">
                                                <input required ng-model="vm.vehicle.colour" ng-class="{'edited':vm.vehicle.colour}" name="Colour" type="color" class="form-control input-block-level" placeholder="Enter Colour.">
                                            </div>

                                            <label class="col-md-2 control-label">Year</label>
                                            <div class="col-md-4">
                                                <input required ng-model="vm.vehicle.year" ng-class="{'edited':vm.vehicle.year}" name="Year" type="text" class="form-control input-block-level" placeholder="Enter Year.">
                                            </div>

                                        </div>

                                        <div class="form-group">
                                            <label class="col-md-2 control-label">Paint Type</label>
                                            <div class="col-md-4">
                                                <select data-ng-options="source.name for source in PaintTypeList" ng-model="vm.vehicle.selectedPaintType" class="form-control"></select>
                                            </div>

                                            <label class="col-md-2 control-label">Vehicle Code</label>
                                            <div class="col-md-4">
                                                <select data-ng-options="source.name for source in VehicleCodeList" ng-model="vm.vehicle.selectedVehicleCode" class="form-control">
                                                    <option value="">---select vehicle code---</option>
                                                </select>
                                            </div>

                                        </div>

                                        <div class="form-group">
                                            <label class="col-md-2 control-label">TV/MM Code</label>
                                            <div class="col-md-4">
                                                <input required ng-model="vm.vehicle.mmCode" ng-class="{'edited':vm.vehicle.mmCode}" name="Colour" type="text" class="form-control input-block-level" placeholder="Enter MMCode.">
                                            </div>

                                            <label class="col-md-2 control-label">Other Information</label>
                                            <div class="col-md-4">
                                                <input required ng-model="vm.vehicle.vehicleOtherInfo" ng-class="{'edited':vm.vehicle.vehicleOtherInfo}" name="Year" type="text" class="form-control input-block-level" placeholder="Other CarInfo">
                                            </div>

                                        </div>

                                        <div class="form-group">
                                            <label class="col-md-2 control-label">Luxury</label>
                                            <div class="col-md-2">
                                                <div class="input-group" style="width: 20% !important;">
                                                    <input type="checkbox" ng-model="vm.vehicle.isLuxury" value="IsLuxury" ng-checked="vm.vehicle.isLuxury=='true'" class="form-control" />
                                                </div>
                                            </div>                               

                                        </div>

                                        <div class="form-group">
                                            <label class="col-md-2 control-label">Specialized</label>
                                            <div class="col-md-2">
                                                <div class="input-group" style="width: 20% !important;">
                                                    <input type="checkbox" ng-model="vm.vehicle.isSpecialisedType" value="IsSpecialisedType" ng-checked="vm.vehicle.isSpecialisedType=='true'" class="form-control" />
                                                </div>
                                            </div>

                                        </div>
                                       
                                    </div>
                                    <div class="form-actions">
                                        <div class="row">
                                            <div class="col-md-offset-10 col-md-9">
                                                <button type="button" class="btn btn-circle green button-submit">Submit</button>
                                            </div>
                                        </div>
                                    </div>

                                    <!-- END FORM-->
                                </div>
                            </div>

                        </div>

                    </div>
                </div>
            </div>
        </div>

        <div id="myModal_autocomplete" class="modal fade" role="dialog" aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header" style="background-color:lightgray;">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                        <h4 class="modal-title">Changes Alert</h4>
                    </div>
                    <div class="modal-body form">
                        <label style="margin:15px 0px 0px 15px;">Do you want to save changes you made in this page?</label>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary" data-dismiss="modal">Continue without saving</button>
                        <button type="button" class="btn btn-primary button-submit">
                            <i class="fa fa-check"></i> Save changes
                        </button>
                    </div>
                </div>
            </div>
        </div>

    </form>

